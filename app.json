{
  "name": "requestbin",
  "description": "Inspect HTTP requests. Debug webhooks.",
  "website": "https://github.com/jennings/requestbin",
  "repository": "https://github.com/jennings/requestbin",
  "logo": "http://requestb.in/static/img/logo-2x.png",
  "keywords": ["python"],
  "env": {
    "REALM": {
      "description": "Set to \"prod\" to store data in Redis",
      "value": "prod"
    },
    "SECRET_KEY": {
      "description": "Secret key for encrypting session data",
      "generator": "secret"
    }
  },
  "addons": [
    "heroku-redis"
  ]
}
